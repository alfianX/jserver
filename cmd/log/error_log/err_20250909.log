                document.addEventListener('DOMContentLoaded', function () {
                    const statusElement = document.getElementById('status-member');
                    if (!statusElement) return;

                    fetch('/header/member')
                        .then(response => response.json())
                        .then(data => {
                            const { subscription, warning_expiry, days_left } = data; // pastikan `days_left` juga dikirim dari backend

                            // Default
                            statusElement.classList.add('text-bg-success');
                            statusElement.innerText = subscription;

                            if (subscription.includes('Nonaktif')) {
                                statusElement.classList.remove('text-bg-success');
                                statusElement.classList.add('text-bg-secondary');
                                statusElement.style.cursor = 'pointer';
                                statusElement.addEventListener('click', () => {
                                    window.location.href = '/pilihpembayaran';
                                });
                            }

                            if (warning_expiry) {
                                // Tambahkan efek visual: border kuning dan ikon notif kecil
                                statusElement.style.border = '2px solid #ffc107';
                                statusElement.style.boxShadow = '0 0 6px #ffc107';
                                statusElement.innerText = subscription;

                                // Tambahkan tanda seru kecil (notifikasi)
                                const notif = document.createElement('span');
                                notif.innerHTML = '<i class="fa fa-exclamation-circle text-warning"></i>';
                                notif.style.position = 'absolute';
                                notif.style.top = '-6px';
                                notif.style.right = '-6px';
                                notif.style.backgroundColor = '#fff';
                                notif.style.borderRadius = '50%';
                                notif.style.fontSize = '12px';
                                notif.style.padding = '2px';

                                // Buat parent-nya relative agar posisinya sesuai
                                statusElement.style.position = 'relative';
                                statusElement.appendChild(notif);

                                // Tambahkan tooltip dengan title
                                statusElement.title = `Membership Anda akan berakhir dalam ${days_left} hari.`;
                            }
                        })
                        .catch(error => {
                            console.error('Error fetching subscription:', error);
                        });
                });
                </script>
                
        
